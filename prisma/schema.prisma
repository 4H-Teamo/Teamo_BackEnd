generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://teamo_pg:teamo_pg@localhost:5432/teamo_pg"
}

model Users {
  user_id  String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  nickname String @unique
}
